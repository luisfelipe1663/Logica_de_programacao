programa
{
    funcao inicio()
    {
        inteiro numero, quadrado
        escreva("Quadrados dos números de 15 a 200:\n\n")
        numero = 15

        faca
        {
            quadrado = numero * numero
            escreva(numero, "² = ", quadrado, "\n")
           numero++
        }
        enquanto (numero <= 200)
    }
}
